<?xml version='1.0' encoding='UTF-8'?>
<ti:app xmlns:ti='http://ti.appcelerator.org'>
<id>com.tidesdk.YotoManager</id>
<name>YotoManager</name>
<version>0.1.0</version>
<publisher>Goliatone</publisher>
<url>http//goliatone.com</url>
<icon>yoto_manager_logo.png</icon>
<copyright>2013 by Goliatone</copyright>
	<!-- Window Definition - these values can be edited -->
	<window>
		<id>initial</id>
		<title>YotoManager</title>
		<url>app://index.html</url>
		<width>1200</width>
		<max-width>3000</max-width>
		<min-width>1200</min-width>
		<height>750</height>
